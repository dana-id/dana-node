{
  "name": "dana-node",
  "version": "1.5.7",
  "author": "dana-id",
  "repository": {
    "type": "git",
    "url": "https://github.com/dana-id/dana-node.git"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./payment_gateway/v1": {
      "import": "./dist/payment_gateway/v1/index.js",
      "require": "./dist/payment_gateway/v1/index.js",
      "types": "./dist/payment_gateway/v1/index.d.ts"
    },
    "./widget/v1": {
      "import": "./dist/widget/v1/index.js",
      "require": "./dist/widget/v1/index.js",
      "types": "./dist/widget/v1/index.d.ts"
    },
    "./disbursement/v1": {
      "import": "./dist/disbursement/v1/index.js",
      "require": "./dist/disbursement/v1/index.js",
      "types": "./dist/disbursement/v1/index.d.ts"
    },
    "./webhook/v1": {
      "import": "./dist/webhook/v1/index.js",
      "require": "./dist/webhook/v1/index.js",
      "types": "./dist/webhook/v1/index.d.ts"
    },
    "./merchant_management/v1": {
      "import": "./dist/merchant_management/v1/index.js",
      "require": "./dist/merchant_management/v1/index.js",
      "types": "./dist/merchant_management/v1/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.js",
      "types": "./dist/runtime.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "payment_gateway/v1": ["./dist/payment_gateway/v1/index.d.ts"],
      "widget/v1": ["./dist/widget/v1/index.d.ts"],
      "disbursement/v1": ["./dist/disbursement/v1/index.d.ts"],
      "webhook/v1": ["./dist/webhook/v1/index.d.ts"],
      "merchant_management/v1": ["./dist/merchant_management/v1/index.d.ts"],
      "runtime": ["./dist/runtime.d.ts"],
      "index": ["./dist/index.d.ts"]
    }
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest --coverage --runInBand"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": ["dist/"]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "dependencies": {
    "date-fns-tz": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.0",
    "jest": "^30.0.3",
    "ts-jest": "^29.3.2",
    "typescript": "^4.0 || ^5.0"
  }
}